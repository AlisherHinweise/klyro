{
    "extends": [
      "next/core-web-vitals",
      "next/typescript",
      "plugin:prettier/recommended"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": 0,
      "no-console": "warn",
      "no-debugger": "warn",
      "@typescript-eslint/ban-ts-comment": "warn",
      "@typescript-eslint/no-unused-vars": "warn"
    }
  }
